DROP TABLE IF EXIST CUSTOMER;
DROP TABLE IF EXIST ACCOUNT;

CREATE TABLE CUSTOMER(
	CUSTOMER_ID INT AUTO_INCREMENT PRIMARY KEY,
	NAME VARCHAR2(100),
	EMAIL VARCHAR2(100),
	PHONE_NO INT,
	CREATED_TS DATE
);

CREATE TABLE ACCOUNT(
	CUSTOMER_ID INT AUTO_INCREMENT PRIMARY KEY,
	NAME VARCHAR2(100),
	EMAIL VARCHAR2(100),
	PHONE_NO INT,
	CREATED_TS DATE
);